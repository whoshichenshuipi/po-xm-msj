root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.{yml,yaml,properties}]
charset = utf-8

[*.java]
charset = utf-8
indent_style = space
indent_size = 4

[*.{js,ts,vue}]
charset = utf-8
indent_style = space
indent_size = 2

[*.md]
charset = utf-8
trim_trailing_whitespace = false

